#!/bin/bash

GREEN='\033[1;32m'
BLUE='\033[1;34m'
RED='\033[1;31m'
YELLOW='\033[1;33m'
RESET='\033[0m'

find -name "*~" -delete
find -name "#*#" -delete
find -name "*.swp" -delete
find -name "*.swo" -delete
find -name "*.gcda" -delete
find -name "*.gcno" -delete
find -name "*.log" -delete
find -name "*.o" -delete

echo -e "${GREEN}âœ… Cleaned up temporary files${RESET}"
